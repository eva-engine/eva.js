{
  "private": true,
  "version": "1.2.8-alpha.0",
  "workspace": "packages",
  "name": "eva.js",
  "scripts": {
    "clean:dist": "rm -rf dist packages/**/dist",
    "clean": "lerna clean && rm -rf node_modules packages/**/package-lock.json && npm run clean:dist",
    "dev": "vite ./examples -c ./examples/vite.config.ts",
    "prebuildDev": "node ./scripts/build.js spine-base -d",
    "buildDev": "node ./scripts/build.js -d",
    "prebuildProd": "node ./scripts/build.js spine-base -p -t",
    "buildProd": "node ./scripts/build.js -p -t",
    "build": "npm run buildDev && npm run buildProd",
    "release": "node ./scripts/release.js",
    "bootstrap": "node ./scripts/bootstrap.js",
    "test": "jest --runInBand",
    "test-cov": "jest --coverage",
    "ci": "lerna bootstrap && npm run cov",
    "cov": "jest --json > node_modules/.tap.json",
    "api": "api-documenter markdown -i temp -o docs/api"
  },
  "keywords": [
    "game",
    "engine",
    "javascript",
    "animation",
    "2d",
    "3d",
    "webgl",
    "canvas",
    "svg"
  ],
