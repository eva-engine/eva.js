{
  "private": true,
  "version": "1.0.5-alpha.1",
  "workspace": "packages",
  "name": "eva.js",
  "scripts": {
    "dev": "node ./scripts/dev.js",
    "build": "node ./scripts/build.js",
    "buildDev": "npm run build -- -a -d -f umd",
    "buildProd": "npm run build -- -a -p -f cjs-esm-umd",
